<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityGiaoDienChinh"
    android:background="@drawable/backgroud">

    <androidx.drawerlayout.widget.DrawerLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        android:id="@+id/drawer_GiaoDienChinh">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="414dp"
                    android:layout_height="120dp"
                    android:layout_marginTop="28dp"
                    app:layout_constraintEnd_toEndOf="@+id/imageView11"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/ic_blue"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="414dp"
                    android:layout_height="120dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/ic_blue_wave"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:id="@+id/imageView14"
                    android:layout_width="174dp"
                    android:layout_height="118dp"
                    android:layout_marginTop="10dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.237"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/ic__38291"
                    tools:ignore="VectorDrawableCompat" />

                <TextView
                    android:id="@+id/txt_1"
                    android:layout_width="wrap_content"
                    android:layout_height="72dp"
                    android:text="24/7"
                    android:textColor="@color/white"
                    android:textSize="57dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.893"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="đặt mọi lúc, ship mọi nơi"
                    android:textColor="@color/white"
                    android:textSize="14dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.937"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txt_1" />

                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbar_GiaoDienChinh"
                    android:layout_width="38dp"
                    android:layout_height="38dp"
                    android:background="@drawable/boder_toolbar"
                    android:minHeight="?attr/actionBarSize"
                    android:theme="?attr/actionBarTheme"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.042"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.145"
                    tools:ignore="MissingConstraints" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:layout_marginLeft="30dp"
                android:layout_marginTop="10dp"
                android:textSize="20dp"
                android:id="@+id/txt_seemore1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Đang được giảm giá...see more>>"
                android:textColor="@color/white"/>

            <androidx.viewpager2.widget.ViewPager2
                android:id="@+id/slide1"
                android:layout_width="match_parent"
                android:layout_height="153dp"
                android:layout_marginTop="15dp"
                android:paddingStart="30dp"
                android:paddingEnd="30dp" />

            <TextView
                android:layout_marginTop="5dp"
                android:layout_marginLeft="30dp"
                android:textSize="20dp"
                android:textColor="@color/white"
                android:id="@+id/txt_seemore2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Đang được yêu thích...see more>>" />

            <androidx.viewpager2.widget.ViewPager2
                android:id="@+id/slide2"
                android:layout_width="match_parent"
                android:layout_height="153dp"
                android:layout_marginTop="15dp"
                android:paddingStart="30dp"
                android:paddingEnd="30dp" />

            <LinearLayout
                android:layout_marginLeft="41dp"
                android:layout_marginTop="17dp"
                android:background="@drawable/boder_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:background="@drawable/boder_layout2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:background="@drawable/boder_layout3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_marginRight="15dp"
                            android:layout_marginBottom="5dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="10dp"
                            android:id="@+id/image_monChinh"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:srcCompat="@drawable/icon1" />

                        <TextView
                            android:textColor="@android:color/black"
                            android:paddingLeft="7dp"
                            android:textSize="12dp"
                            android:id="@+id/textView4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Món chính" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:background="@drawable/boder_layout4"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_marginBottom="5dp"
                            android:id="@+id/image_doUong"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginRight="15dp"
                            app:srcCompat="@drawable/icon2" />

                        <TextView
                            android:paddingLeft="12dp"
                            android:textColor="@android:color/black"
                            android:textSize="12dp"
                            android:id="@+id/textView5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Đồ uống" />
                    </LinearLayout>

                    <LinearLayout
                        android:background="@drawable/boder_layout4"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_marginBottom="5dp"
                            android:layout_marginRight="15dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="10dp"
                            android:id="@+id/image_kfc"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:srcCompat="@drawable/icon3" />

                        <TextView
                            android:paddingLeft="18dp"
                            android:textSize="12dp"
                            android:textColor="@android:color/black"
                            android:id="@+id/textView6"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="K.F.C" />
                    </LinearLayout>

                    <LinearLayout
                        android:background="@drawable/boder_layout5"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_marginRight="15dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginBottom="5dp"
                            android:layout_marginTop="10dp"
                            android:id="@+id/image_combo"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:srcCompat="@drawable/icon7" />

                        <TextView
                            android:paddingLeft="15dp"
                            android:textSize="12dp"
                            android:textColor="@android:color/black"
                            android:id="@+id/textView10"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Combo" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:background="@drawable/boder_layout6"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:background="@drawable/boder_layout7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_marginRight="15dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginBottom="5dp"
                            android:layout_marginTop="10dp"
                            android:id="@+id/image_monPhu"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:srcCompat="@drawable/icon4" />

                        <TextView
                            android:paddingLeft="12dp"
                            android:textColor="@android:color/black"
                            android:textSize="12dp"
                            android:id="@+id/textView7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Món phụ" />
                    </LinearLayout>

                    <LinearLayout
                        android:background="@drawable/boder_layout4"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_marginRight="15dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginBottom="5dp"
                            android:layout_marginTop="10dp"
                            android:id="@+id/image_anVat"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:srcCompat="@drawable/icon5" />

                        <TextView
                            android:paddingLeft="18dp"
                            android:textSize="12dp"
                            android:textColor="@android:color/black"
                            android:id="@+id/textView8"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Ăn vặt" />
                    </LinearLayout>

                    <LinearLayout
                        android:background="@drawable/boder_layout4"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_marginRight="15dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginBottom="5dp"
                            android:layout_marginTop="10dp"
                            android:id="@+id/image_khaiVi"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:srcCompat="@drawable/icon6" />

                        <TextView
                            android:paddingLeft="15dp"
                            android:textColor="@android:color/black"
                            android:textSize="12dp"
                            android:id="@+id/textView9"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Khai vị" />
                    </LinearLayout>

                    <LinearLayout
                        android:background="@drawable/boder_layout8"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="15dp"
                            android:layout_marginBottom="5dp"
                            android:layout_marginTop="10dp"
                            android:id="@+id/image_healthy"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:srcCompat="@drawable/icon8" />

                        <TextView
                            android:paddingLeft="14dp"
                            android:textColor="@android:color/black"
                            android:textSize="12dp"
                            android:id="@+id/textView11"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Healthy" />
                    </LinearLayout>
                </LinearLayout>


            </LinearLayout>

        </LinearLayout>

        <com.google.android.material.navigation.NavigationView
            app:itemTextColor="@android:color/white"
            app:itemIconTint="@android:color/white"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            android:fitsSystemWindows="true"
            android:id="@+id/navigation_GiaoDienChinh"
            app:headerLayout="@layout/navigation_header"
            app:menu="@menu/navigation_menu"
            android:background="#99141414">

        </com.google.android.material.navigation.NavigationView>
    </androidx.drawerlayout.widget.DrawerLayout>

</androidx.constraintlayout.widget.ConstraintLayout>